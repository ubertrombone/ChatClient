CREATE TABLE chats (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    userOne TEXT NOT NULL,
    userTwo TEXT NOT NULL
);

selectAll:
SELECT *
FROM chats;

selectChatByID:
SELECT *
FROM chats
WHERE id = ?;

selectChat:
SELECT *
FROM chats
WHERE userOne = :userOne AND userTwo = :userTwo OR userOne = :userTwo AND userTwo = :userOne;

insertChat:
INSERT INTO chats(userOne, userTwo)
VALUES (:userOne, :userTwo);

updateChat:
UPDATE chats
SET userOne = :userOne, userTwo = :userTwo
WHERE id = :id;

deleteChat:
DELETE FROM chats WHERE id = :id;